<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox styleClass="theme-selector" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="perococco.perobobbot.frontfx.gui.fxml.ThemeSelectorController">
    <ComboBox fx:id="themeList"/>
    <Button fx:id="applyThemeButton" mnemonicParsing="false" text="%_lit.Apply-Theme" onAction="#applyTheme"/>
</VBox>
